{
  "name": "@mfbhatt/azure-push-notifications",
  "version": "1.0.14",
  "description": "The Push Notifications API provides access to native Azure push notifications for Capacitor.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CapacitorAzurePushNotifications.podspec",
    "MfbhattAzurePushNotifications.podspec"
  ],
  "author": "mfbhatt@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfbhatt/capacitor-azure-push-notification.git"
  },
  "bugs": {
    "url": "https://github.com/mfbhatt/capacitor-azure-push-notification/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api AzurePushNotificationsPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^5.2.3",
    "@capacitor/cli": "^5.2.3",
    "@capacitor/core": "^5.2.3",
    "@capacitor/docgen": "0.2.0",
    "@capacitor/ios": "^5.2.3",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "~1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "eslint": "^7.32.0",
    "prettier": "~2.8.3",
    "prettier-plugin-java": "~2.0.0",
    "rimraf": "^4.1.1",
    "rollup": "^2.79.1",
    "swiftlint": "^1.0.1",
    "typescript": "~4.9.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.2.3"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
